class Apple {
    field int x;
    field int y;
    
    constructor Apple new(int coordX, int coordY) {
        let x = coordX;
        let y = coordY;

        return this;
    }

    method int getX() {
        return x;
    }

    method int getY() {
        return y;
    }

    method void setX(int newX) {
        let x = newX;
        return;
    }

    method void setY(int newY) {
        let y = newY;
        return;
    }
}